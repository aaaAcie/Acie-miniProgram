/* pages/map/map.wxss */
.map_container {
  width: 100vw;
  height: 100vh;
}
.search {
  position: fixed;
  z-index: 999;
  top: 0;
  left: 0;
  width: 100%;
  height: 60px;
}
.search-list {
  position: absolute;
  z-index: 998;
  top: 60px;
  left: 0;
  margin: 0;
}
.map {
  width: 100%;
  height: 100%;
}
.des {
  position: fixed;
  left: 0;
  bottom: 0;
  height: 80px;
  width: 100vw;
  padding: 10px;
  box-sizing: border-box;
  background-color: #fff;
  box-shadow: 0px -5px 5px #ededed;
  border-radius: 30px 30px 0 0;
}
.search-text {
  color: #504b5070;
  font-size: 15px;
}