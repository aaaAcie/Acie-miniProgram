<!--pages/logs/camera/camera.wxml-->
<camera device-position="back" flash="off" binderror="error" style="width: 100%; height: 300px; margin-bottom: 20px;"></camera>
<button type="default" bindtap="takePhoto" style="width: 100%;">拍照</button>
<view>预览</view>
<image mode="widthFix" src="{{src}}" style="width: 100%; border-radius: 50rpx;"></image>